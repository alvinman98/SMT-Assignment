<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="seq_travel_advance" model="ir.sequence">
            <field name="name">Travel Advance Sequence</field>
            <field name="code">travel.advance.sequence</field>
            <field name="prefix">TR/%(y)s/%(month)s/</field>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_other_advance" model="ir.sequence">
            <field name="name">Other Advance Sequence</field>
            <field name="code">other.advance.sequence</field>
            <field name="prefix">OR/%(y)s/%(month)s/</field>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_travel_setlement" model="ir.sequence">
            <field name="name">Travel Settlement Sequence</field>
            <field name="code">travel.settlement.sequence</field>
            <field name="prefix">TE/%(y)s/%(month)s/</field>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_other_settlement" model="ir.sequence">
            <field name="name">Other Settlement Sequence</field>
            <field name="code">other.settlement.sequence</field>
            <field name="prefix">OE/%(y)s/%(month)s/</field>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False" />
        </record>
    </data>
</odoo>